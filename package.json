        "eslint-plugin-promise": "^4.2.1",
        "esm": "^3.2.25",
        "htmlhint": "^0.11.0",
        "jsdoc": "^3.6.2",
        "jsdom": "^15.1.1",
        "jsonlint": "^1.6.3",
        "markdownlint": "^0.15.0",
        "metalint": "^0.8.3",
        "mocha": "^6.1.4",
        "node-fetch": "^2.6.0",
        "nyc": "^14.1.1",
        "purgecss": "^1.3.0",
        "stylelint": "^10.1.0",
        "stylelint-order": "^3.0.1",
        "yaml-lint": "^1.2.4"
    }

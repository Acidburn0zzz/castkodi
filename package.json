        "eslint-plugin-promise": "^4.1.1",
        "esm": "^3.2.22",
        "htmlhint": "^0.10.3",
        "jsdoc": "^3.6.1",
        "jsdom": "^15.0.0",
        "jsonlint": "^1.6.3",
        "markdownlint": "^0.14.0",
        "metalint": "^0.8.2",
        "mocha": "^6.1.4",
        "node-fetch": "^2.5.0",
        "nyc": "^14.1.0",
        "purgecss": "^1.3.0",
        "stylelint": "^10.0.1",
        "stylelint-order": "^3.0.0",
        "yaml-lint": "^1.2.4"
    }
